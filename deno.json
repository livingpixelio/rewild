{
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check",
    "start": "deno run -A  --watch=static/,routes/ --unstable-kv dev.ts",
    "build": "deno run -A  dev.ts build",
    "preview": "deno serve -A _fresh/server.js",
    "update": "deno run -A -r jsr:@fresh/update .",
    "test": "deno test -A"
  },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "exclude": ["**/_fresh/*"],
  "imports": {
    "fresh": "jsr:@fresh/core@^2.0.2",
    "preact": "npm:preact@^10.27.1",
    "@preact/signals": "npm:@preact/signals@^2.3.1",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^1.0.2",
    "vite": "npm:vite@^7.1.3",
    "tailwindcss": "npm:tailwindcss@^4.1.10",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.12",
    "$std/": "https://deno.land/std@0.216.0/",
    "ptera": "https://deno.land/x/ptera@v1.0.2/mod.ts",
    "ptera/": "https://deno.land/x/ptera@v1.0.2/",
    "assert/": "https://deno.land/std@0.221.0/assert/",
    "foblog": "https://deno.land/x/foblog@0.0.7/mod.ts"
  },
  "compilerOptions": { "jsx": "react-jsx", "jsxImportSource": "preact" },
  "nodeModulesDir": "auto"
}
